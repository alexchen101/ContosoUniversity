@page
@model ContosoUniversity.Pages.AboutModel
@{
    ViewData["Title"] = "Student Body Stastistics";
}

<h2>Student Body Stastistics</h2>

<table>
    <tr>
        <th>Enrollment Date</th>
        <th>Students</th>
    </tr>

    @foreach (var item in Model.Students)
    {
        <tr>
            <td>
                @Html.DisplayFor(m => item.EnrollmentDate)
            </td>
            <td>
                @Html.DisplayFor(m => item.StudentCount)
            </td>
        </tr>
    }

</table>